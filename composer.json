{
    "name": "weblabel/api-bundle",
    "description": "This bundle provides few functionalities that helps you quickly develop REST API based on Symfony framework",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Adam Kowalski",
            "email": "adam.kowalski@weblabel.pl"
        }
    ],
    "require": {
        "php": ">=7.4",
        "symfony/framework-bundle": "^5.4",
        "symfony/security-bundle": "^5.4",
        "symfony/yaml": "^5.4",
        "symfony/form": "^5.4",
        "symfony/validator": "^5.4",
        "weblabel/data-transformer-bundle": "^1.0.2"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^5.4",
        "phpstan/phpstan": "^0.12.99",
        "phpstan/phpstan-symfony": "^0.12.44",
        "friendsofphp/php-cs-fixer": "^3.3"
    },
    "autoload": {
        "psr-4": {
            "Weblabel\\ApiBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Weblabel\\ApiBundle\\Tests\\": "tests/"
        }
    }
}
